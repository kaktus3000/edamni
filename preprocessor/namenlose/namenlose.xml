<?xml version="1.0" encoding="UTF-8"?>

<!--http://www.hifi-forum.de/viewthread-267-592.html-->

<!DOCTYPE horn SYSTEM "../horn.dtd">

<horn dx="0.005">
	<wall id="rear_wall">
		<A2>.137</A2>
		<damping_thickness>0.09</damping_thickness>
		<damping_transient>0.05</damping_transient>
		<damping_constant>17100</damping_constant>
		<neighbor2 ref="rear_chamber" />
	</wall>

	<conical id="rear_chamber">
		<length>.21</length>
		<A1>.137</A1>
		<A2>.137</A2>
		<damping_constant>0.0</damping_constant>
		<neighbor1 ref="rear_wall" />
		<neighbor2 ref="speaker" />
	</conical>

	<speaker id="speaker">
		<A1>0.35</A1>
		<A2>0.0855</A2>
		<type>Monacor_SP-38A500BS</type>
		<neighbor1 ref="rear_chamber" />
		<neighbor2 ref="pressure_chamber" />
	</speaker>

	<conical id="pressure_chamber">
		<length>.025</length>
		<A1>.0855</A1>
		<A2>.0855</A2>
		<damping_constant>0.0</damping_constant>
		<neighbor1 ref="speaker" />
		<neighbor2 ref="nozzle" />
	</conical>

	<mic id="chamber_mic">
		<neighbor2 ref="pressure_chamber" />
	</mic>
	
	<conical id="nozzle">
		<length>.01</length>
		<A1>.0855</A1>
		<A2>.04</A2>
		<damping_constant>0.0</damping_constant>
		<neighbor1 ref="pressure_chamber" />
		<neighbor2 ref="horn" />
	</conical>

	<conical id="horn">
		<length>2.2</length>
		<A1>.04</A1>
		<A2>.4</A2>
		<damping_constant>0.0</damping_constant>
		<neighbor1 ref="nozzle" />
		<neighbor2 ref="throat" />
	</conical>
	
	<space id="throat">
		<A1>.4</A1>
		<fraction>0.25</fraction>
		<length>1.0</length>
		<neighbor1 ref="horn" />
	</space>

	<mic id="SPL">
		<neighbor2 ref="throat" />
	</mic>

</horn>
